<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<navigation-rule>
	    <from-view-id>/loginAdm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>homeAdmin</from-outcome>
            <to-view-id>/admin/produto.xhtml</to-view-id>
        </navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	    <from-view-id>/admin/produto.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginAdmin</from-outcome>
            <to-view-id>/loginAdm.xhtml</to-view-id>
        </navigation-case>
	</navigation-rule>

</faces-config>
